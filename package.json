  "engines": {
    "node": "^10.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && echo \"Executing tsc...\" && yarn run tsc && yarn run flow"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "packages/*/src/**/*.js": [
      "yarn run prettier --write",
      "git add",
      "yarn run lint:js"
    ],
    "packages/*/src/**/*.ts": [
      "yarn run prettier --write",
      "git add",
      "yarn run test --findRelatedTests --passWithNoTests -w 4",
      "yarn run lint:ts"
    ]
  },
  "name": "neotracker",
  "private": true,
  "repository": "https://github.com/neotracker/neotracker",
  "resolutions": {
    "ajv": "6.5.2",
    "bn.js": "4.11.8",
    "grpc": "1.13.0",
    "react": "16.4.1",
    "react-dom": "16.4.1"
  },
  "scripts": {
    "develop": "yarn run ts-node ./packages/neotracker-build/src/develop.ts",
    "e2e": "yarn run ts-node ./packages/neotracker-server-test/src/cypress/run.ts",
    "e2e-ci": "yarn run e2e --ci",
    "flow": "flow",
    "lint": "yarn run lint:ts && yarn run lint:js:all",
    "lint-staged": "lint-staged",
    "lint:js": "eslint",
    "lint:js:all": "yarn run lint:js \"packages/*/src/**/*.js\"",
    "lint:ts": "tslint --project .",
    "nit": "yarn run nit:source",
    "nit:source": "yarn run nit:source:ts && yarn run nit:source:js",
    "nit:source:js": "prettier --list-different \"packages/*/src/**/*.js\"",
    "nit:source:ts": "prettier --list-different \"packages/*/src/**/*.ts\"",
    "prettier:all": "yarn run prettier:source",
    "prettier:source": "yarn run prettier:source:ts && yarn run prettier:source:js",
    "prettier:source:js": "prettier --write \"packages/*/src/**/*.js\"",
    "prettier:source:ts": "prettier --write \"packages/*/src/**/*.ts\"",
    "test": "jest --config jest.unit.js",
    "test-ci": "yarn run test -w 1 --passWithNoTests",
    "test-coverage": "yarn run test --coverage",
    "ts-node": "cross-env TS_NODE_PROJECT=tsconfig.cli.json node -r ts-node/register/transpile-only --max-old-space-size=8192",
    "tsc": "tsc"
  },
  "version": "0.0.0",
  "workspaces": [
    "packages/neotracker-build",
    "packages/neotracker-client-web",
    "packages/neotracker-core",
    "packages/neotracker-server-db",
    "packages/neotracker-server-graphql",
    "packages/neotracker-server-scrape",
    "packages/neotracker-server-scrape-check",
    "packages/neotracker-server-test",
    "packages/neotracker-server-utils",
    "packages/neotracker-server-utils-koa",
    "packages/neotracker-server-web",
    "packages/neotracker-shared-graphql",
    "packages/neotracker-shared-utils",
    "packages/neotracker-shared-web"
  ]
}